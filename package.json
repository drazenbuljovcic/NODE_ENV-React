{
  "name": "react-starter-env",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "prebuild:dev": "npm run clean",
    "build:dev": "BUILD=true NODE_ENV=development webpack --config config/webpack.dev-build.js --display-error-details --progress --colors",
    "start:dev": "NODE_ENV=development webpack-dev-server --config config/webpack.dev-server.js --inline --hot --progress --colors",
    "prebuild:prod": "npm run clean",
    "build:prod": "BUILD=true NODE_ENV=production webpack -p --config config/webpack.prod.js --progress --colors",
    "start:server": "NODE_ENV=production node server/server.js",
    "start": "npm run build:prod && npm run start:server",
    "analyze-bundle": "NODE_ENV=production webpack -p --config config/webpack.prod.js --progress --colors "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zendra1994/NODE_ENV-React.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zendra1994/NODE_ENV-React/issues"
  },
  "homepage": "https://github.com/zendra1994/NODE_ENV-React#readme",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "preact-router": "^2.5.2",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-server": "^4.2.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
